apiVersion: extensions/v1beta1
kind: Deployment
metadata:
  name: unprivileged-integration-test
  namespace: <%= namespace %>
spec:
  replicas: 1
  template:
    metadata:
      labels:
        app: unprivileged-integration-test
    spec:
      containers:
        - name: nginx
          image: bitnami/nginx
          imagePullPolicy: "IfNotPresent"
          ports:
            - containerPort: 5432

