apiVersion: extensions/v1beta1
kind: Deployment
metadata:
  name: privileged-integration-test
  namespace: <%= namespace %>
spec:
  replicas: 1
  template:
    metadata:
      labels:
        app: privileged-integration-test
    spec:
      containers:
        - name: postgres
          image: postgres:10.4
          imagePullPolicy: "IfNotPresent"
          ports:
            - containerPort: 5432

